snippet inproceedings "inproceedings"
@inproceedings{$1,
  author    = "$2",
  title     = "$3",
  booktitle = "$4",
  series    = "$4",
  year      = $5,
  pages     = "$6",
  publisher = "$7",
  address   = "$8"
}
endsnippet


snippet misc "misc"
@misc{$1,
  author       = "$2",
  title        = "$3",
  howpublished = "$4",
  year         = $5,
  note         = "$8"
}
endsnippet
